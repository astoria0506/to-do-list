generator client {
  provider = "prisma-client-js"
  // output   = "./generated/prisma-client-js"
  // binaryTargets   = ["rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
}